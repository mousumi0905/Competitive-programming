#Given two strings s and t,
# return true if s is a subsequence of t, or false otherwise.
class Solution:
    def isSubsequence(self, s: str, t: str) -> bool:
        n1,n2 = len(s),len(t)
        i,j = 0,0
        while (i < n1 and j < n2):
            if (s[i] == t[j]):
                 i += 1
            j += 1
        return i == n1
     
